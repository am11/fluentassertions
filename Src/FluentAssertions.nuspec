<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>FluentAssertions</id>
    <title>Fluent Assertions</title>
    <version>4.13.1</version>
    <owners>Dennis Doomen</owners>
    <authors>Dennis Doomen</authors>
    <summary>
      Fluent API for asserting the results of unit tests for .NET Framework 4.5.1+ and .NET Standard 1.4+.
      Supports the unit test frameworks MSTest, MSTest2, NUnit, XUnit, XUnit2, MBUnit, Gallio, MSpec, and NSpec.
    </summary>
    <description>
      A very extensive set of extension methods that allow you to more naturally specify the expected outcome of a TDD or
      BDD-style unit tests. Runs on .NET Framework 4.5.1+ and .NET Standard 1.4+.
      Supports the unit test frameworks MSTest, MSTest2, NUnit, XUnit, XUnit2, MBUnit, Gallio, MSpec, and NSpec.
    </description>
    <language>en-US</language>
    <licenseUrl>https://github.com/fluentassertions/fluentassertions/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://www.fluentassertions.com</projectUrl>
    <tags>MSTest MSTest2 xUnit xUnit2 NUnit MSpec NSpec Gallio MbUnit TDD BDD Fluent netcore netstandard uwp</tags>
    <copyright>Copyright Dennis Doomen 2010-2017</copyright>
    <releaseNotes>
      See https://github.com/fluentassertions/fluentassertions/releases/
    </releaseNotes>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Xml" targetFramework="net451" />
      <frameworkAssembly assemblyName="System.Xml.Linq" targetFramework="net451" />
    </frameworkAssemblies>
    <dependencies>
      <group targetFramework="netstandard1.4">
        <dependency id="NETStandard.Library" version="1.6.1" />
        <dependency id="System.Reflection.TypeExtensions" version="4.3.0" />
        <dependency id="System.Reflection.Emit.Lightweight" version="4.3.0" />
      </group>
      <group targetFramework="netstandard1.6">
        <dependency id="NETStandard.Library" version="1.6.1" />
        <dependency id="System.Reflection.TypeExtensions" version="4.3.0" />
        <dependency id="System.Reflection.Emit.Lightweight" version="4.3.0" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="System.Configuration.ConfigurationManager" version="4.4.0" />
        <dependency id="System.Reflection.Emit" version="4.3.0" />
        <dependency id="System.Reflection.Emit.Lightweight" version="4.3.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src=".\FluentAssertions\bin\Release\net451\*.*"  target="lib\net451" />
    <file src=".\FluentAssertions\bin\Release\netstandard1.4\FluentAssertions.*" exclude="**\*.json" target="lib\netstandard1.4" />
    <file src=".\FluentAssertions\bin\Release\netstandard1.6\FluentAssertions*.*" exclude="**\*.json" target="lib\netstandard1.6" />
    <file src=".\FluentAssertions\bin\Release\netstandard2.0\FluentAssertions*.*" exclude="**\*.json" target="lib\netstandard2.0" />
  </files>
</package>
